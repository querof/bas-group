{
    "require": {
        "php": "^8.1",
        "symfony/routing": "^6.1",
        "symfony/http-foundation": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/config": "^6.1",
        "symfony/yaml": "^6.1",
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^3.2",
        "symfony/orm-pack": "^2.4",
        "symfony/doctrine-bridge": "^6.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
